# Configuration Firebase pour Dental Treatment Planner
# Copiez ce fichier vers .env et remplissez avec vos vraies clés Firebase

# Clés Firebase (remplacez par vos vraies clés)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:your-app-id

# Développement local (optionnel)
# Définir à 'true' pour utiliser les émulateurs Firebase en développement
VITE_USE_FIREBASE_EMULATOR=false

# Configuration des émulateurs (si utilisés)
VITE_FIREBASE_EMULATOR_HOST=localhost
VITE_FIREBASE_FIRESTORE_PORT=8080
VITE_FIREBASE_AUTH_PORT=9099